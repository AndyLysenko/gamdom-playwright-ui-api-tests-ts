{
  "name": "gamdom-playwright-ts",
  "version": "1.0.0",
  "description": "Demo test project of gamdom.com",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "test:api": "npx jest ./api-tests/specs",
    "test:ui": "playwright test",
    "test:ui:ui": "playwright test --ui",
    "test:ui:headed": "playwright test --headed",
    "test:ui:chrome": "playwright test --project=chromium",
    "test:ui:headed:chrome": "playwright test --project=chromium --headed",
    "test:ui:firefox": "playwright test --project=firefox",
    "test:ui:headed:firefox": "playwright test --project=firefox --headed",
    "test:ui:debug": "PWDEBUG=1 playwright test",
    "test:ui:ci": "playwright test --reporter=dot",
    "test:ui:clean": "rm -rf test-results/",
    "test:ui:generate-report": "npx playwright show-report html-report"
  },
  "keywords": [
    "e2e",
    "playwright",
    "test automation",
    "ui testing",
    "jest",
    "api testing",
    "jira api"
  ],
  "author": "Andrii Lysenko",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.43.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-playwright": "^1.5.2",
    "eslint-plugin-promise": "^6.1.1",
    "openapi-typescript-codegen": "^0.29.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1"
  }
}
